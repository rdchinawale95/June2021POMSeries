
public class sample2 {

	public static void main(String[] args) {
			
		String s="learning java";
		
		String[] a= s.split(" ");
		
		String rev="";
	
		for (int i = 0; i < a.length; i++) {
			for (int j=a[i].length()-1; j >0 ; j--) {
				rev=rev+a[i];
			}
			
		}
		System.out.println(rev);
	}

}
