<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Regression Test" verbose="4" parallel="tests" thread-count="3">


	<test name="Amazon">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://www.amazon.in/" />

		<classes>
			<class name="testNGpractice.AmazonTestt" />
		</classes>

	</test>

	<test name="Google">
		<parameter name="browser" value="firefox" />
		<parameter name="url" value="https://www.google.com/" />

		<classes>
			<class name="testNGpractice.GooglTest" />
		</classes>

	</test>

	<test name="Rediff Mail Test">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://www.rediff.com/" />

		<classes>
			<class name="testNGpractice.RediffMailTest" />
		</classes>

	</test>


</suite>